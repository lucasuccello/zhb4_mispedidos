            </l:content>
        </l:VerticalLayout>
        <buttons>
			<Button text="Cancelar" press="onCancelarNuevoLote" type="Default"/>
			<Button press="onGuardarNuevo" type="Accept" icon="sap-icon://save" tooltip="Guardar y generar nuevo Lote"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>

